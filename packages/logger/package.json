{
  "name": "@vscode-logging/logger",
  "version": "1.0.0",
  "description": "Logger Library for VSCode Extensions",
  "keywords": [
    "vscode",
    "logger"
  ],
  "main": "lib/api.js",
  "repository": "https://github.com/sap/vscode-logging/",
  "license": "Apache-2.0",
  "typings": "./api.d.ts",
  "dependencies": {
    "@types/vscode": "^1.41.0",
    "@vscode-logging/types": "^0.1.2",
    "fast-safe-stringify": "2.0.7",
    "fs-extra": "8.1.0",
    "lodash": "4.17.15",
    "moment-timezone": "0.5.27",
    "stacktrace-js": "2.0.2",
    "streamroller": "2.2.3",
    "triple-beam": "1.3.0",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.0",
    "proxyquire": "2.1.3"
  },
  "scripts": {
    "ci": "npm-run-all type-check coverage:*",
    "test": "mocha \"./test/**/*spec.js\"",
    "coverage:run": "nyc mocha \"./test/**/*spec.js\"",
    "coverage:check": "nyc check-coverage --lines 100 --branches 100 --statements 100 --functions 100",
    "type-check": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
